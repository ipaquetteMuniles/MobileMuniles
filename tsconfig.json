{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/FormButton.js",
    "app/(tabs)/index.js",
    "components/FormInput.js",
    "app/(tabs)/profil.js",
    "app/(tabs)/signup.js"  ]
}